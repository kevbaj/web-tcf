<h1>Edit Contact Us Info</h1>
<form method="post" action="/updatecontact">
    <div class="mb-3">
		<label for="addr" class="form-label">Address</label>
		<input type="text" class="form-control" name="addr" id="addr" aria-describedby="emailHelp">
	</div>
	<div class="mb-3">
		<label for="city" class="form-label">City</label>
		<input type="text" class="form-control" name="city" id="city" aria-describedby="emailHelp">
	</div>
	<div class="mb-3">
		<label for="jpos" class="form-label">Province</label>
		<input type="text" class="form-control" name="jpos" id="jpos" aria-describedby="emailHelp">
	</div>
    <div class="mb-3">
		<label for="fname" class="form-label">Country</label>
		<input type="text" class="form-control" name="fname" id="fname" aria-describedby="emailHelp">
	</div>
	<div class="mb-3">
		<label for="lname" class="form-label">Postal Code</label>
		<input type="text" class="form-control" name="lname" id="lname" aria-describedby="emailHelp">
	</div>
	<div class="mb-3">
		<label for="jpos" class="form-label">Phone Number</label>
		<input type="text" class="form-control" name="jpos" id="jpos" aria-describedby="emailHelp">
	</div>
    	<label for="jpos" class="form-label">Email Address</label>
		<input type="text" class="form-control" name="jpos" id="jpos" aria-describedby="emailHelp">
	</div>
</form>