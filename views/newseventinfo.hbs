{{#each ne_res}}
<div class="row mt-3">
    <div class="col">
        <h3 class="display-6 text-center">{{ne_title}}</h3>
        <div class="text-center">
            <small><i>Posted on {{dtname}}</i></small>
        </div>
    </div>
</div>
<div class="row mt-2">
    <div class="col">
        <div class="text-center">
            <img class="img rounded img-fluid" src="{{ne_image}}" >
        </div>
    </div>
</div>
<div class="row mt-5">
    <div class="col">
        <div class="text-center">
            {{ne_det}}
        </div>
    </div>
</div>
{{/each}}